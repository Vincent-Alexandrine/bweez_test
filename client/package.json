{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boilerplate with webpack react and SC",
  "main": "src/index.js",
  "repository": "git@github.com:Vincent-Alexandrine/boilerplate-webpack.git",
  "author": "Vincent Alexandrine",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer dist/main.js dist/main.js.map",
    "build": "webpack --mode production --config ./webpack/index.js",
    "lint": "eslint \"src/\" --ext \".js\" --ext \".jsx\" --ignore-pattern \".test.jsx.snap\"",
    "start": "webpack-dev-server --open --mode development --config ./webpack/index.js",
    "test": "jest -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "favicons-webpack-plugin": "^0.0.9",
    "fetch-mock": "^7.3.3",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-styled-components": "^6.3.1",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "dependencies": {
    "i18next": "^14.0.1",
    "modern-normalize": "^0.5.0",
    "polished": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-i18next": "10.0.0",
    "react-inlinesvg": "^0.8.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^1.8.0",
    "styled-components": "^4.1.3"
  }
}
